{
  "name": "demo-create-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.10.0",
    "pnpm": ">=8.14.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "start": "vite",
    "start:fresh": "vite --force",
    "preview": "vite preview --host",
    "build": "pnpm --parallel type-check && vite build",
    "lint": "eslint . --ext .vue,.ts",
    "lint:fix": "eslint . --fix --ext .vue,.ts",
    "format": "prettier --check src/ index.html",
    "format:fix": "prettier --write src/ index.html",
    "type-check": "vue-tsc --build --force"
  },
  "dependencies": {
    "@earwurm/utilities": "workspace:*",
    "earwurm": "workspace:*",
    "vue": "^3.4.8"
  },
  "devDependencies": {
    "@earwurm/types": "workspace:*",
    "@rushstack/eslint-patch": "^1.6.1",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.10.8",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vue-tsc": "^1.8.27"
  }
}
