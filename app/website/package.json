{
  "name": "website",
  "version": "0.1.0",
  "description": "Marketing and test app for Earwurm library.",
  "author": "Curtis Dulmage",
  "license": "UNLICENSED",
  "keywords": [
    "marketing",
    "app",
    "earwurm"
  ],
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.13.0",
    "pnpm": ">=9.1.0"
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf *.tsbuildinfo",
    "start": "vite",
    "start:fresh": "vite --force",
    "preview": "vite preview --host",
    "build": "pnpm --parallel type-check && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false"
  },
  "dependencies": {
    "beeftools": "^0.1.3",
    "earwurm": "workspace:*",
    "vue": "^3.4.27"
  },
  "devDependencies": {
    "@earwurm/types": "workspace:*",
    "@eslint/eslintrc": "^3.1.0",
    "@tsconfig/node20": "^20.1.4",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint-plugin-vue": "^9.26.0",
    "lightningcss": "^1.25.1",
    "vite-plugin-svg-sprite": "^0.5.2",
    "vue-tsc": "^2.0.19"
  }
}
