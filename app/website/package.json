{
  "name": "website",
  "type": "module",
  "version": "0.2.1",
  "private": true,
  "description": "Marketing and test app for Earwurm library.",
  "author": "Curtis Dulmage",
  "license": "UNLICENSED",
  "keywords": [
    "marketing",
    "app",
    "earwurm"
  ],
  "engines": {
    "node": ">=22.11.0",
    "pnpm": ">=9.14.4"
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf *.tsbuildinfo",
    "start": "vite",
    "start:fresh": "vite --force",
    "preview": "vite preview --host",
    "build": "pnpm --parallel type-check && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "vue-tsc --noEmit -p tsconfig.json --composite false"
  },
  "dependencies": {
    "beeftools": "^0.1.8",
    "earwurm": "workspace:*",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@earwurm/types": "workspace:*",
    "@tsconfig/node22": "^22.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.16.0",
    "eslint-plugin-format": "^0.1.3",
    "lightningcss": "^1.28.2",
    "vite-plugin-svg-sprite": "^0.5.2",
    "vue-tsc": "^2.1.10"
  }
}
